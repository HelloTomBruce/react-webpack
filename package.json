    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot --inline --content-base . --history-api-fallback --config ./build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.conf.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "build-dll": "webpack --config ./build/webpack_dll.config.js --mode production",
    "eslint": "eslint src/**/*.js",
    "stylelint": "stylelint src/**/*.less --fix",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "antd": "^3.13.2",
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.18.0",
    "formik": "^1.5.7",
    "graphql": "^14.2.1",
    "graphql-tag": "^2.10.1",
    "immutable": "^4.0.0-rc.12",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1"
  },
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 8",
    "iOS >= 8",
    "Android >= 4"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.{css,json,md}": [
      "git add"
    ],
    "*.js": [
      "prettier --tab-width 4 --write",
      "eslint --fix",
      "git add"
    ],
    "*.less": [
      "stylelint --fix"
    ]
  }
}
